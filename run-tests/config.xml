<?xml version='1.0' encoding='utf-8'?>
<widget id="com.photokandy.PKVideoThumbnail.runtests" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>runTests</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="cdvtests/index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="~4.0.1" />
    <engine name="android" spec="~5.1.0" />
    <plugin name="cordova-plugin-test-framework" spec="http://git-wip-us.apache.org/repos/asf/cordova-plugin-test-framework.git" />
    <plugin name="cordova-plugin-photokandy-video-thumbnail" spec=".." />
    <plugin name="cordova-plugin-photokandy-video-thumbnail-tests" spec="../tests" />
    <plugin name="cordova-plugin-device" spec="~1.1.1" />
</widget>
